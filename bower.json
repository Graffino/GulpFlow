    "lodash": {
      "main": "dist/lodash.js"
    },
    "pointer-events-poly": {
      "main": "pointer_events_polyfill.js"
    },
    "h5Validate": {
      "main": "jquery.h5validate.js"
    },
    "fontfaceobserver": {
      "main": "fontfaceobserver.js"
    },
    "select2": {
      "main": [
        "dist/css/select2.css",
        "dist/js/select2.js"
      ]
    },
    "simple-jquery-datepicker": {
      "main": [
        "dist/datepicker.css",
        "dist/datepicker.js"
      ]
    },
    "vue": {
      "main": "dist/vue.min.js"
    },
    "csv": {
      "main": "lib/csv.js"
    }
  },
  "dependencies": {
    "vue": "^2.5.13"
  }
