  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "normalize-css": "^7.0.0",
    "browser-update": "~2.0.2",
    "fontfaceobserver": "~2.0.13",
    "h5Validate": "~0.9.0",
    "jquery": "~3.3.1",
    "jquery.browser": "^0.1.0",
    "jquery-migrate": "^3.0.0",
    "js-cookie": "^2.2.0",
    "magnific-popup": "~1.1.0",
    "pointer-events-poly": "*",
    "select2": "~4.0.5",
    "velocity": "~1.5.1",
    "svg4everybody": "^2.1.9",
    "farbtastic": "^2.0.0-alpha.1",
    "ui-sortable": "*",
    "csv": "^1.0.4",
    "simple-jquery-datepicker": "^0.6.4",
    "ramda": "^0.25.0",
    "Babel-Polyfill-Standalone": "babel-polyfill-standalone#^6.26.0",
    "lodash": "^4.17.4",
    "vue": "^2.5.13"
  },
  "overrides": {
