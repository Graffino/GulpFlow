{
  "name": "gulp-flow",
  "version": "3.1.1",
  "description": "Gulp Flow we use at Graffino.",
  "main": [
    "www/css/main.css",
    "www/js/main.js"
  ],
  "keywords": [
    "gulp",
    "flow",
    "bootstrap"
  ],
  "homepage": "http://graffino.com",
  "author": {
    "name": "Graffino",
    "email": "hello@graffino.com"
  },
  "license": "MIT",
  "private": false,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "browser-update": "~2.0.1",
    "fontfaceobserver": "~2.0.9",
    "h5Validate": "~0.9.0",
    "jquery": "~3.2.1",
    "jquery-migrate": "^3.0.0",
    "js-cookie": "^2.1.4",
    "magnific-popup": "~1.1.0",
    "nunjucks": "^3.0.0",
    "pointer-events-poly": "*",
    "select2": "~4.0.3",
    "slick.js": "~1.6.0",
    "velocity": "~1.5.0",
    "svg4everybody": "^2.1.8"
  },
  "overrides": {
    "browser-update": {
      "dependencies": {
        "jquery": "~3.2.1"
      }
    },
    "pointer-events-poly": {
      "main": "pointer_events_polyfill.js"
    },
    "srcset-poly": {
      "main": "build/srcset.js"
    },
    "h5Validate": {
      "main": "jquery.h5validate.js"
    },
    "slick.js": {
      "main": [
        "slick/slick.js",
        "slick/slick.css"
      ]
    },
    "fontfaceobserver": {
      "main": "fontfaceobserver.js"
    },
    "select2": {
      "main": [
        "dist/css/select2.css",
        "dist/js/select2.js"
      ]
    }
  }
}
