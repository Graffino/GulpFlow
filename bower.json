{
  "name": "gulp-flow",
  "version": "2.2.7",
  "description": "Gulp Flow we use at Graffino.",
  "main": [
    "www/assets/css/main.css",
    "www/assets/js/main.js"
  ],
  "keywords": [
    "gulp",
    "flow",
    "bootstrap"
  ],
  "homepage": "http://graffino.com",
  "author": {
    "name": "Graffino",
    "email": "hello@graffino.com"
  },
  "license": "MIT",
  "private": false,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "jquery": "~3.0.0",
    "h5Validate": "~0.9.0",
    "browser-update": "~1.3.0",
    "fontfaceobserver": "~1.7.1",
    "srcset-poly": "*",
    "slick.js": "~1.6.0",
    "matchHeight": "~0.7.0",
    "velocity": "~1.2.3",
    "magnific-popup": "~1.1.0",
    "pointer-events-poly": "*",
    "select2": "~4.0.3",
    "jquery-placeholder": "~2.3.1",
    "jquery.browser": "^0.1.0",
    "handlebars": "^4.0.5",
    "jquery-migrate": "^3.0.0"
  },
  "resolutions": {
    "jquery": "~3.0.0"
  },
  "overrides": {
    "pointer-events-poly": {
      "main": "pointer_events_polyfill.js"
    },
    "srcset-poly": {
      "main": "build/srcset.js"
    },
    "h5Validate": {
      "main": "jquery.h5validate.js"
    },
    "slick.js": {
      "main": [
        "slick/slick.js",
        "slick/slick.css"
      ]
    },
    "fontfaceobserver": {
      "main": "fontfaceobserver.js"
    },
    "select2": {
      "main": [
        "dist/css/select2.css",
        "dist/js/select2.js"
      ]
    }
  }
}
