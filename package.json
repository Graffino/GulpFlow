{
  "name": "gulp-flow",
  "version": "4.2.0",
  "title": "Gulp Flow",
  "description": "Gulp Flow we use at Graffino.",
  "homepage": "http://gulp-flow.dev",
  "devpage": "http://gulp-flow.dev",
  "author": {
    "name": "Graffino",
    "url": "http://graffino.com"
  },
  "repository": {
    "type": "Git",
    "url": "https://github.com/Graffino/GulpFlow.git"
  },
  "keywords": [],
  "contributors": [],
  "bugs": "https://github.com/Graffino/GulpFlow.git",
  "license": "MIT",
  "private": false,
  "engines": {},
  "xo": {
    "space": 2,
    "globals": [
      "$",
      "_",
      "window",
      "document",
      "localStorage",
      "console",
      "FontFaceObserver",
      "PointerEventsPolyfill",
      "nunjucks",
      "$graffino",
      "Cookies",
      "iScroll",
      "IScroll",
      "svg4everybody",
      "btoa",
      "wp",
      "ajaxurl",
      "R",
      "Vue",
      "csv",
      "FormData"
    ],
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": 2,
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "no-extend-native": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-negated-condition": 0,
      "max-lines": 0,
      "one-var": 0,
      "new-cap": 0,
      "no-warning-comments": 0,
      "max-params": 0,
      "quote-props": [
        0,
        "consistent-as-needed",
        {
          "keywords": true
        }
      ],
      "no-prototype-builtins": 0,
      "no-extra-bind": 0,
      "max-depth": [
        "error",
        {
          "max": 10
        }
      ],
      "no-return-assign": 0,
      "no-else-return": 0,
      "babel/new-cap": 0,
      "import/no-unresolved": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "backstopjs": "^3.0.38",
    "bower": "^1.8.2",
    "browser-sync": "^2.23.5",
    "browsersync": "0.0.1-security",
    "bs-html-injector": "^3.0.3",
    "bs-latency": "^1.0.0",
    "critical": "^1.1.0",
    "csswring": "^6.0.2",
    "debounce": "1.1.0",
    "del": "^3.0.0",
    "es6-promise": "^4.2.2",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-unicorn": "^3.0.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-bump": "~3.0.0",
    "gulp-combine-mq": "~0.4.0",
    "gulp-composer": "^0.4.4",
    "gulp-concat": "2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-exec": "^3.0.1",
    "gulp-file": "0.4.0",
    "gulp-filter": "~5.1.0",
    "gulp-git": "^2.5.1",
    "gulp-group-concat": "^1.1.6",
    "gulp-htmlhint": "^1.0.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-hub": "frankwallis/gulp-hub#4.1.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "~4.1.0",
    "gulp-inject": "^4.3.0",
    "gulp-load-plugins": "~1.5.0",
    "gulp-modernizr": "~1.0.0-alpha",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-optimize-js": "^1.1.0",
    "gulp-order": "^1.1.1",
    "gulp-phplint": "^0.4.1",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "~7.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-stylint": "~4.0.1",
    "gulp-stylus": "~2.7.0",
    "gulp-svg-sprite": "~1.3.7",
    "gulp-ttf2woff": "^1.1.1",
    "gulp-ttf2woff2": "^2.0.2",
    "gulp-uglify": "~3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-xo": "^0.16.1",
    "htmlhint-stylish": "^1.0.3",
    "imagemin-mozjpeg": "~7.0.0",
    "imagemin-pngquant": "~5.0.1",
    "imagemin-svgo": "^6.0.0",
    "jeet": "7.2.0",
    "lighthouse": "^2.8.0",
    "main-bower-files": "~2.13.1",
    "npm-check-updates": "^2.14.0",
    "patternlab-node": "^2.12.0",
    "postcss-quantity-queries": "0.5.0",
    "print": "^1.0.2",
    "rupture": "0.7.1",
    "styleguidekit-assets-default": "^3.5.2",
    "styleguidekit-mustache-default": "^3.1.0",
    "stylint": "^1.5.9",
    "stylint-json-reporter": "^0.3.1",
    "stylint-stylish": "^1.4.0",
    "syncy": "^2.2.2",
    "tree-kill": "^1.2.0",
    "xo": "^0.18.2"
  },
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "production": "./node_modules/gulp/bin/gulp.js --env production",
    "staging": "./node_modules/gulp/bin/gulp.js --env staging",
    "development": "./node_modules/gulp/bin/gulp.js --env development",
    "debug": "./node_modules/gulp/bin/gulp.js --env development --debug true",
    "clean": "./node_modules/gulp/bin/gulp.js clean",
    "critical": "./node_modules/gulp/bin/gulp.js critical",
    "bump": "./node_modules/gulp/bin/gulp.js bump",
    "lint": "./node_modules/gulp/bin/gulp.js lint",
    "lighthouse": "lighthouse",
    "regression-config": "backstop genConfig",
    "regression-reference": "backstop reference",
    "regression-bless": "backstop bless",
    "regression-test": "backstop test",
    "regression-report": "backstop openReport",
    "check-npm-updates": "ncu",
    "check-bower-updates": "ncu -m bower",
    "update-npm-modules": "ncu -u && npm install",
    "update-bower-modules": "ncu -m bower -u && bower install"
  }
}
