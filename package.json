{
  "name": "gulp-flow",
  "version": "2.5.4",
  "title": "Gulp Flow",
  "description": "Gulp Flow we use at Graffino.",
  "homepage": "http://gulp-flow.dev",
  "devpage": "http://gulp-flow.dev",
  "author": {
    "name": "Graffino",
    "url": "http://graffino.com"
  },
  "repository": {
    "type": "Git",
    "url": "https://github.com/Graffino/GulpFlow.git"
  },
  "keywords": [],
  "contributors": [],
  "bugs": "https://github.com/Graffino/GulpFlow.git",
  "license": "MIT",
  "private": false,
  "engines": {},
  "xo": {
    "space": 4,
    "globals": [
      "$",
      "window",
      "document",
      "localStorage",
      "console",
      "FontFaceObserver",
      "PointerEventsPolyfill",
      "nunjucks"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "max-lines": 0,
      "one-var": 0,
      "max-params": 0,
      "no-warning-comments": 0,
      "quote-props": [0, "consistent-as-needed", {"keywords": true}]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "backstopjs": "^1.3.5",
    "critical": "^0.7.3",
    "csswring": "^5.1.0",
    "debounce": "1.0.0",
    "del": "^2.2.1",
    "es6-promise": "^3.2.1",
    "fs": "0.0.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-bump": "~2.2.0",
    "gulp-combine-mq": "~0.4.0",
    "gulp-concat": "2.6.0",
    "gulp-copy": "~0.0.2",
    "gulp-cssmin": "0.1.7",
    "gulp-data": "^1.2.1",
    "gulp-exec": "^2.1.2",
    "gulp-file": "0.3.0",
    "gulp-filter": "~4.0.0",
    "gulp-git": "^1.8.0",
    "gulp-group-concat": "^1.1.5",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "~3.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-jshint": "~2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "~1.2.4",
    "gulp-modernizr": "~1.0.0-alpha",
    "gulp-notify": "^2.2.0",
    "gulp-nunjucks": "^2.2.0",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-order": "^1.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "~6.1.1",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rimraf": "^0.2.0",
    "gulp-sequence": "~0.4.5",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-stylint": "~3.0.0",
    "gulp-stylus": "~2.5.0",
    "gulp-svg-sprite": "~1.3.3",
    "gulp-ttf2woff": "^1.1.0",
    "gulp-ttf2woff2": "^2.0.2",
    "gulp-uglify": "~1.5.4",
    "gulp-uncss": "^1.0.5",
    "gulp-util": "^3.0.7",
    "gulp-xo": "^0.12.0",
    "htmlhint-stylish": "^1.0.3",
    "imagemin-mozjpeg": "~6.0.0",
    "imagemin-pngquant": "~5.0.0",
    "jeet": "6.1.2",
    "main-bower-files": "~2.13.1",
    "minimist": "1.2.0",
    "normalize.styl": "3.0.3",
    "npm-check-updates": "^2.6.8",
    "postcss-quantity-queries": "0.4.0",
    "rupture": "0.6.1",
    "stylint-stylish": "^1.3.2",
    "xo": "^0.16.0"
  },
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "production": "./node_modules/gulp/bin/gulp.js build --env production",
    "staging": "./node_modules/gulp/bin/gulp.js build --env staging",
    "development": "./node_modules/gulp/bin/gulp.js build --env development",
    "debug": "./node_modules/gulp/bin/gulp.js build --env development --debug true",
    "clean": "./node_modules/gulp/bin/gulp.js clean",
    "critical": "./node_modules/gulp/bin/gulp.js critical",
    "bump": "./node_modules/gulp/bin/gulp.js bump",
    "regression-config": "gulp genConfig --cwd ./node_modules/backstopjs",
    "regression-reference": "gulp reference --cwd ./node_modules/backstopjs",
    "regression-bless": "gulp bless --cwd ./node_modules/backstopjs",
    "regression-test": "gulp test --cwd ./node_modules/backstopjs",
    "regression-report": "gulp openReport --cwd ./node_modules/backstopjs",
    "check-npm-updates": "ncu",
    "check-bower-updates": "ncu -m bower",
    "update-npm-modules": "ncu -u && npm install",
    "update-bower-modules": "ncu -m bower -u && bower install"
  }
}
