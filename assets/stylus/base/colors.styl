/**
 * Backgrounds
 */

$color-background             = #FFFFFF;


/**
 * Text
 */

$color-text                   = black;


/**
 * Heading
 */

$color-heading                = black;


/**
 * Default Links
 */

$color-link-default           = $color-text;
$color-link-default-hover     = $color-text;


/**
 * Defaults
 */

$default-color                = $color-text;
